<section>
  <h2>Crie sua própria sala</h2>
  <form action="/create-room" method="POST">
    <!-- Label apenas para robôs de busca -->
    <label class="sr-only" for="room-pass "> Insira uma senha </label>
    <!-- Formulario -->
    <input
      type="password"
      name="password"
      id="room-pass"
      placeholder="Insira uma senha"
    />
    <button>
      <img src="/images/users-light.svg" alt="Icone de entrar numa sala" />
      Criar sala
    </button>
  </form>
</section>
